{
  "name": "angular-modal-gallery",
  "version": "5.1.12",
  "description": "Modal image gallery for Angular >=4",
  "main": "src/main.ts",
  "license": "MIT",
  "author": "Stefano Cappa",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "homepage": "https://github.com/geethanga/angular-modal-gallery#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --app dev",
    "build:lib": "npm run clean:all && ng-packagr && npm run copy:other && cpr dist/angular-modal-gallery src/angular-modal-gallery && npm run build:demo",
    "build:demo": "cpr dist/angular-modal-gallery demo/systemjs/node_modules/angular-modal-gallery && cpr dist/angular-modal-gallery demo/webpack/node_modules/angular-modal-gallery && cpr dist/angular-modal-gallery demo/angular-cli/node_modules/angular-modal-gallery",
    "clean": "rimraf dist",
    "clean:all": "npm run clean && rimraf src/angular-modal-gallery && rimraf demo/systemjs/node_modules/angular-modal-gallery demo/webpack/node_modules/angular-modal-gallery demo/angular-cli/node_modules/angular-modal-gallery",
    "clean-coverage": "rimraf coverage",
    "copy:other": "cpr README.md dist/angular-modal-gallery/README.md && cpr LICENSE dist/angular-modal-gallery/LICENSE && cpr CHANGELOG.md dist/angular-modal-gallery/CHANGELOG.md && cpr DEVELOPER.md dist/angular-modal-gallery/DEVELOPER.md",
    "pretest": "npm run clean-coverage",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "docs": "typedoc --out docs angular-modal-gallery/src",
    "compodoc": "compodoc -p ./tsconfig.compodoc.json",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "mousetrap": "^1.6.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@compodoc/compodoc": "^1.0.3",
    "@types/jasmine": "^2.6.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/mousetrap": "^1.5.33",
    "@types/node": "^8.0.47",
    "codelyzer": "^3.2.2",
    "cpr": "^3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.5.0-rc.1",
    "protractor": "^5.2.0",
    "rimraf": "^2.6.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "~2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ks89/angular-modal-gallery.git"
  },
  "bugs": {
    "url": "https://github.com/Ks89/angular-modal-gallery/issues"
  }
}
